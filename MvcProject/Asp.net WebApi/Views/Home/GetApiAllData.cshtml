
@{
    ViewBag.Title = "GetApiAllData";
}

<h2>GetApiAllData</h2>


<table id="table-1">
    <thead>
        <tr>
            <th>
                Employee Id
            </th>
            <th>
                EmployeeName
            </th>
            <th>
                EmployeeSalary
            </th>
        </tr>
    </thead>
</table>
<br />
<br />


<script src="~/Scripts/jquery-1.10.2.min.js"></script>

<script>
    onload = function showdata() {
        $.ajax({
            url: "http://localhost:44387/api/EmployeeModels",
            type: "Get",
            datatype: "json",
            success: function (result) {

                alert(result);
                $.each(result, function (i, key) {


                    $("#table-1 tr:last").after('<tr><td>' + key.Empid + '</td><td>' + key.EmpName + '</td><td>' + key.EmpSalary + '</td></tr>');

                });

            }
        });
    }
</script>